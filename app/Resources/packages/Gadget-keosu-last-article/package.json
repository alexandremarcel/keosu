{
   "name" : "keosu-last-article",
   "description" : "Display a list of articles in your app",
   "version" : "0.1",
   "type" : "gadget",
   "param" : [
      {"name": "articlesPerPage", "type" : "choice","options": {
         "choices" : {"3":"3","5":"5","10":"10","50":50,"100":"100"},
         "required" : 1
      }},
      {"name" : "tag", "type" : "text", "options":{
      	"required" : 0
	  }},
      { "name" : "cache", "type" : "number", "options": {
         "label": "Cache in minute",
         "data": 10}},
      { "name" : "timeout", "type" : "number", "options": {
         "label": "Timeout in seconde",
         "data": 10}}
   ],
   "require" : [
      { "name" : "keosu-base" , "version" : ">=0.1" },
      { "name" : "keosu-comments" , "version" : ">=0.1" },
      { "name" : "keosu-share" , "version" : ">=0.1" }
   ]
}
