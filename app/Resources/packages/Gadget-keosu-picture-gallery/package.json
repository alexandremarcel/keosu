{
    "name" : "keosu-picture-gallery",
    "description" : "Create a menu gadget",
    "version" : "0.1",
    "type" : "gadget",
    "param" : [
        {"name" : "picturesPerPage" , "type" : "choice" , "options" : {
            "required" : 1,
            "choices" : {"3":"3","5":"5","10":"10","20":"20","40":"40","50":"50","100":"100"}
        }},
        {"name" : "tag", "type" : "text", "options":{
            "required" : 0
        }},
        { "name" : "cache", "type" : "number", "options": {
            "label": "Cache in minute",
            "data": 10}},
        { "name" : "timeout", "type" : "number", "options": {
            "label": "Timeout in seconde",
            "data": 10}}
    ],
    "require" : [
        { "name" : "keosu-base" , "version" : ">=0.1" },
        { "name" : "keosu-comments" , "version" : ">=0.1" }
    ]
}
