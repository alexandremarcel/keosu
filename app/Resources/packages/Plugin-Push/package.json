{
  "name": "keosu-push",
  "description": "This allow to send push message to your users",
  "version": "0.1",
  "type": "plugin",
  "require": [
    {
      "name": "angularJs",
      "version": ">=0.1"
    }
  ],
  "configCordova": [
    {
      "preference": [
        {
          "@attributes": {
            "name": "android-build-tool",
            "value": "gradle"
          }
        }
      ]
    },
    {
      "plugin": [
        {
          "@attributes": {
            "name": "phonegap-plugin-push",
            "spec": "~1.4.5",
            "source": "npm"
          }
        }
      ]
    }
  ],
  "pluginToInstall": [
    "cordova plugin add https://github.com/phonegap/phonegap-plugin-push.git"
  ],
  "appParam": [
    {
      "name": "enablePush",
      "type": "checkbox",
      "options": {
        "required": 0
      }
    },
    {
      "name": "googleGcmProjectNumber",
      "type": "text",
      "options": {
        "required": 0
      }
    },
    {
      "name": "googleGcmApiKey",
      "type": "text",
      "options": {
        "required": 0
      }
    },
    {
      "name": "googleUseMultiCurl",
      "type": "checkbox",
      "options": {
        "required": 0
      }
    },
    {
      "name": "iosSandbox",
      "type": "checkbox",
      "options": {
        "required": 0
      }
    },
    {
      "name": "iosPem",
      "type": "file",
      "options": {
        "required": 0,
        "mapped": 0
      }
    },
    {
      "name": "iosPassPhrase",
      "type": "password",
      "options": {
        "required": 0
      }
    }
  ]
}
