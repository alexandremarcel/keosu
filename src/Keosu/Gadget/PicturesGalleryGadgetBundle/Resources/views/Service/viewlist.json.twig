{% spaceless %}
{ "data": [
	{% for picture in pictures %}
		{
			"id" : "{{ loop.index }}",
			"dataModelObjectName" : "{{ picture.dataModelObjectName }}",
			"name" : "{{ picture.name }}",
			"description" : "{{ picture.description }}",
			"orientation" : "{{ picture.orientation }}",
			"path" : "{{ url_base }}{{ picture.webPath }}",
			"thumbpath" : "{{ url_base }}{{ picture.thumbWebPath }}",
			"enableComments" : "{{ picture.enableComments }}"
		}
		{% if loop.last ==false%}
			,
		{% endif %}
	{% endfor %}
],
"picturesperpage":"{{picturesperpage}}" }
{% endspaceless %}
